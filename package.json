{
  "version": "0.1.8",
  "name": "responsive-image-react",
  "author": "Jonathan Sparvath",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsparvath/responsive-image-react"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "dev": "node ./scripts/dev.js",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/responsive-image-react.esm.js",
  "size-limit": [
    {
      "path": "dist/responsive-image-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/responsive-image-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@size-limit/preset-small-lib": "^4.6.2",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@types/hls.js": "^0.13.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.1",
    "husky": "^4.3.0",
    "node-sass": "^5.0.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-is": "^17.0.1",
    "rollup-plugin-postcss": "^3.1.8",
    "size-limit": "^4.6.2",
    "style-loader": "^2.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "hls.js": "^0.14.17"
  }
}
